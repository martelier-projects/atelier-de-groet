{
  "name": "personal-website",
  "version": "0.0.1",
  "private": false,
  "scripts": {
    "dev": "next dev",
    "watch": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "deploy": "yarn build && yarn export",
    "format": "prettier --write .",
    "lint:js": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:css": "npx stylelint '**/*.scss'",
    "lint": "yarn run lint:js && yarn run lint:css",
    "test": "jest",
    "start:cms": "npx netlify-cms-proxy-server",
    "make-flexible": "node node_modules/@martijnnieuwenhuizen/generate-component ./components/flexibles $1 --with-netlify-cms",
    "make-module": "node node_modules/@martijnnieuwenhuizen/generate-component ./components/modules $1",
    "make-partial": "node node_modules/@martijnnieuwenhuizen/generate-component ./components/partials $1"
  },
  "dependencies": {
    "focus-trap-react": "^11.0.4",
    "gray-matter": "^4.0.3",
    "next": "14.2.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-headroom": "^3.2.1",
    "react-intersection-observer": "^10.0.0",
    "react-markdown": "^10.1.0",
    "slugify": "^1.6.6"
  },
  "devDependencies": {
    "@martijnnieuwenhuizen/generate-component": "^2.0.0",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.3",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "babel-jest": "^30.2.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.11.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "frontmatter-markdown-loader": "^3.7.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-dom": "^4.0.0",
    "prettier": "^3.7.4",
    "sass": "^1.97.1",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "stylelint-scss": "^4.6.0",
    "typescript": "^5.9.3"
  },
  "packageManager": "npm@10.9.0"
}
